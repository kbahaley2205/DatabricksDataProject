{
  "datasets": [
    {
      "name": "ef6fc1e5",
      "displayName": "LocationData",
      "queryLines": [
        "select latitude,\n",
        "longitude,\n",
        "'GHG emissions mtons CO2e' as emissions\n",
        " \n",
        "from emissions_data"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "ccbaa023",
      "displayName": "TopCountyEmissionsPerPerson",
      "queryLines": [
        "select county_state_name,\n",
        "       population,\n",
        "       cast(\n",
        "         regexp_replace(\n",
        "           `GHG emissions mtons CO2e`,\n",
        "           ',',\n",
        "           ''\n",
        "         )\n",
        "         as decimal(18,2)\n",
        "       ) / cast(\n",
        "         regexp_replace(\n",
        "           population,\n",
        "           ',',\n",
        "           ''\n",
        "         )\n",
        "         as bigint\n",
        "       ) as emissions_per_person\n",
        "from emissions_data\n",
        "order by emissions_per_person desc\n",
        "\n"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "41e81171",
      "displayName": "StateEmissions",
      "queryLines": [
        "select state_abbr,\n",
        "       \n",
        "       sum(  cast(\n",
        "         regexp_replace(\n",
        "           `GHG emissions mtons CO2e`,\n",
        "           ',',\n",
        "           ''\n",
        "         )\n",
        "         as decimal(18,2)\n",
        "       )) \n",
        "      as Total_emissions\n",
        "from emissions_data\n",
        "group by state_abbr \n",
        "order by Total_emissions desc\n",
        "limit 10\n"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "1d779c6a",
      "displayName": "CountyEmissions",
      "queryLines": [
        "select county_state_name,\n",
        "       population,\n",
        "       cast(\n",
        "         regexp_replace(\n",
        "           `GHG emissions mtons CO2e`,\n",
        "           ',',\n",
        "           ''\n",
        "         )\n",
        "         as decimal(18,2)\n",
        "       ) \n",
        "       as Total_emissions\n",
        "from emissions_data\n",
        "order by Total_emissions desc\n",
        "limit 10\n"
      ],
      "catalog": "emissions",
      "schema": "default"
    }
  ],
